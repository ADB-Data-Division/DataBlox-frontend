# Generate a secret with `npx auth secret`
# and replace the value below with it
AUTH_SECRET="your-auth-secret-here"

# Add secrets for your auth providers to the .env.local file
AUTH0_CLIENT_ID=your-auth0-client-id
AUTH0_CLIENT_SECRET=your-auth0-client-secret
AUTH0_ISSUER=https://your-domain.auth0.com

# Backend API URL
NEXT_PUBLIC_BACKEND_URL=http://localhost:2020

# LogRocket Configuration
# Set to 'true' to enable LogRocket in development mode
# It's automatically enabled in production
NEXT_PUBLIC_LOGROCKET_ENABLED=false